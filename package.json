{
  "name": "maria-fifi",
  "version": "1.0.0",
  "description": "Seller monolith",
  "main": "src/backend/app.ts",
  "scripts": {
    "dev:backend": "nodemon --exec ts-node src/backend/app.ts",
    "dev:frontend": "npm run dev --prefix src/frontend",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:frontend": "npm run build --prefix src/frontend",
    "start": "node dist/backend/app.js"
  },
  "author": "ef1_fuchs",
  "license": "ISC",
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.8.1",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
